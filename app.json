    "GOOGLE_CLIENT_SECRET": {
      "description": "Google client secret",
      "value": ""
    },
    "APP_URL": {
      "description": "URL of your app - e.g. https://your-app-name.herokuapp.com",
      "value": ""
    }
  }
}
