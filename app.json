  "keywords": ["facebook", "bandit", "ruby", "react"],
  "addons": [
    "heroku-redis",
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/ruby"
    }
  ],
  "env": {
